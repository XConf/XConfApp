{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends" : [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base",
    "react-app"
  ],
  "rules" : {
    "semi": ["error", "never"],
    "arrow-body-style": "warn"
  }
}
