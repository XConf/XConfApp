require 'json'
require 'time'

build_config_file_path = File.join(Dir.pwd, '..', 'build-config.json')
build_config_file = File.read(build_config_file_path)
build_config = JSON.parse(build_config_file)

ios_certificates_git_url = build_config['ios']['certificatesGitUrl']

git_url(ios_certificates_git_url)

type("development") # The default type, can be: appstore, adhoc, enterprise or development

